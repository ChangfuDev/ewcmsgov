<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>	
	<package name="scheduling.jobclass" extends="struts-default" namespace="/scheduling/jobclass">
		<action name="index">
			<result>/WEB-INF/page/scheduling/jobclass/index.jsp</result>
		</action>
		<action name="delete" class="com.ewcms.scheduling.web.JobClassAction" method="delete"/>
		<action name="query" class="com.ewcms.scheduling.web.JobClassQueryAction" method="query"/>
		<action name="input" class="com.ewcms.scheduling.web.JobClassAction" method="input">
			<result name="input">/WEB-INF/page/scheduling/jobclass/edit.jsp</result>
		</action>
		<action name="save" class="com.ewcms.scheduling.web.JobClassAction" method="save">
			<result>/WEB-INF/page/scheduling/jobclass/edit.jsp</result>
			<result name="input">/WEB-INF/page/scheduling/jobclass/edit.jsp</result>
		</action>
	</package>
	<package name="scheduling.jobinfo" extends="struts-default" namespace="/scheduling/jobinfo">
		<action name="index">
			<result>/WEB-INF/page/scheduling/jobinfo/index.jsp</result>
		</action>
		<action name="delete" class="com.ewcms.scheduling.web.JobInfoAction" method="delete"/>
		<action name="query" class="com.ewcms.scheduling.web.JobInfoQueryAction" method="query"/>
		<action name="input" class="com.ewcms.scheduling.web.JobInfoAction" method="input">
			<result name="input">/WEB-INF/page/scheduling/jobinfo/edit.jsp</result>
		</action>
		<action name="save" class="com.ewcms.scheduling.web.JobInfoAction" method="save">
			<result>/WEB-INF/page/scheduling/jobinfo/edit.jsp</result>
			<result name="input">/WEB-INF/page/scheduling/jobinfo/edit.jsp</result>
		</action>
		<action name="pause" class="com.ewcms.scheduling.web.JobInfoAction" method="pauseJob">
			<result>/WEB-INF/page/scheduling/jobinfo/index.jsp</result>
		</action>
		<action name="resumed" class="com.ewcms.scheduling.web.JobInfoAction" method="resumedJob">
			<result>/WEB-INF/page/scheduling/jobinfo/index.jsp</result>
		</action>
		<!-- 
		<action name="allJobClassList" class="com.ewcms.web.action.scheduling.job.JobAction" method="getAllJobClassList"/>
		 -->
	</package>
	<package name="scheduling.jobchannel" extends="struts-default" namespace="/scheduling/jobchannel">
		<action name="index" class="com.ewcms.scheduling.web.JobChannelAction" method="getJobChannel">
			<result name="input">/WEB-INF/page/scheduling/jobinfo/edit_channel.jsp</result>
		</action>
		<action name="save" class="com.ewcms.scheduling.web.JobChannelAction" method="saveJobChannel">
			<result>/WEB-INF/page/scheduling/jobinfo/edit_channel.jsp</result>
			<result name="input">/WEB-INF/page/scheduling/jobinfo/edit_channel.jsp</result>
		</action>
	</package>
	<package name="scheduling.jobcrawler" extends="struts-default" namespace="/scheduling/jobcrawler">
		<action name="index" class="com.ewcms.scheduling.web.JobCrawlerAction" method="getJobCrawler">
			<result name="input">/WEB-INF/page/scheduling/jobinfo/edit_crawler.jsp</result>
		</action>
		<action name="save" class="com.ewcms.scheduling.web.JobCrawlerAction" method="saveJobCrawler">
			<result>/WEB-INF/page/scheduling/jobinfo/edit_crawler.jsp</result>
			<result name="input">/WEB-INF/page/scheduling/jobinfo/edit_crawler.jsp</result>
		</action>
	</package>
</struts>	