			<result name="index">/WEB-INF/page/plugin/leadingwindow/leader/index.jsp</result>
		</action>
		<action name="delete" class="com.ewcms.web.action.plugin.leadingwindow.leader.LeaderAction" method="delete"/>
	    <action name="query" class="com.ewcms.web.action.plugin.leadingwindow.leader.QueryAction" method="query"/>	
        <action name="input" class="com.ewcms.web.action.plugin.leadingwindow.leader.LeaderAction" method="input">
            <result name="input">/WEB-INF/page/plugin/leadingwindow/leader/edit.jsp</result>
        </action>
        <action name="save" class="com.ewcms.web.action.plugin.leadingwindow.leader.LeaderAction" method="save">
            <result>/WEB-INF/page/plugin/leadingwindow/leader/edit.jsp</result>
