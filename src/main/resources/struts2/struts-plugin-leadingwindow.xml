             <result name="input">/WEB-INF/page/plugin/leadingwindow/leaderchannel/edit.jsp</result>
        </action>
	</package>
	<package name="plugin.leadingwindow.refarticle" extends="struts-default" namespace="/plugin/leadingwindow/refarticle">
		<action name="index">
			<result>/WEB-INF/page/plugin/leadingwindow/refarticle/index.jsp</result>
		</action>
