		<action name="query" class="com.ewcms.web.action.document.article.QueryAction" method="query"/>
		<action name="input" class="com.ewcms.web.action.document.article.ArticleAction" method="input">
			<result name="input">/WEB-INF/page/document/article/edit.jsp</result>
		</action>
		<action name="save" class="com.ewcms.web.action.document.article.ArticleAction" method="save">
			<result>/WEB-INF/page/document/article/edit.jsp</result>
